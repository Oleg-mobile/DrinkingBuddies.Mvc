@using DrinkingBuddies.Mvc.Services.Members.Dto;
@model IEnumerable<MemberDto>

	<table>
		@foreach (MemberDto member in Model)
		{
			<tr>
				<td>@($"Имя - {member.Name}")</td>
				<td>
					<form asp-controller="Members" asp-action="Delete" method="post" asp-route-id="@member.Id">
						<input type="submit" value="Удалить" />
					</form>
				</td>

			</tr>
		}
</table>